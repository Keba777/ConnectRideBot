#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/iojs" ]; then
  exec "$basedir/iojs" --harmony --es_staging "$basedir/../jo/bin/jo" "$@"
else 
  exec iojs --harmony --es_staging "$basedir/../jo/bin/jo" "$@"
fi
